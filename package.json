{
  "name": "@autoguru/icons",
  "version": "0.0.0-semantically-released",
  "description": "Icons and illustrations that support product at AutoGuru",
  "license": "MIT",
  "files": [
    "dist",
    "icons"
  ],
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "yarn run generate && concurrently \"tsc -p tsconfig/cjs.json\" \"tsc -p tsconfig/esm.json\" && tsc --emitDeclarationOnly --declarationDir dist/types -d",
    "clean": "rimraf lib/*.tsx dist/",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|md)\"",
    "generate": "node scripts/build",
    "lint": "eslint \"scripts/**/*.{js,jsx,ts,tsx}\" --cache --quiet",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends @autoguru/commitlint-config",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --cache --quiet --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@autoguru/babel-preset": "^1.0.81",
    "@autoguru/commitlint-config": "^1.0.54",
    "@autoguru/eslint-plugin": "^1.0.81",
    "@autoguru/tsconfig": "^1.0.77",
    "@babel/core": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/github": "^5.5.5",
    "@semantic-release/npm": "^5.3.4",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@svgr/core": "^4.3.3",
    "@svgr/plugin-jsx": "^4.3.3",
    "@svgr/plugin-prettier": "^4.3.2",
    "change-case": "^4.0.1",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.0.0",
    "dedent": "^0.7.0",
    "eslint": "^6.7.2",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "husky": "^3.1.0",
    "kleur": "^3.0.3",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.31",
    "svgo": "^1.3.2",
    "typescript": "~3.7.2"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "volta": {
    "node": "12.12.0",
    "yarn": "1.20.0"
  }
}
