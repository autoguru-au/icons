{
  "name": "@autoguru/icons",
  "version": "0.0.0-semantically-released",
  "description": "Icons and illustrations that support product at AutoGuru",
  "license": "MIT",
  "files": [
    "dist/",
    "icons/"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/icons.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "yarn run generate && tsc",
    "clean": "rimraf lib/*.tsx",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|md)\"",
    "generate": "node scripts/build",
    "lint": "eslint \"scripts/**/*.{js,jsx,ts,tsx}\" --cache --quiet",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends @autoguru/commitlint-config",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --cache --quiet --fix",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@autoguru/babel-preset": "^1.0.69",
    "@autoguru/commitlint-config": "^1.0.54",
    "@autoguru/eslint-plugin": "^1.0.70",
    "@autoguru/tsconfig": "^1.0.63",
    "@babel/core": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@svgr/core": "^4.3.2",
    "@svgr/plugin-jsx": "^4.3.2",
    "@svgr/plugin-prettier": "^4.3.2",
    "change-case": "^3.1.0",
    "cheerio": "^1.0.0-rc.3",
    "dedent": "^0.7.0",
    "eslint": "^6.4.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "husky": "^3.0.5",
    "kleur": "^3.0.3",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "puppeteer": "^1.20.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.21",
    "svgo": "^1.3.0",
    "typescript": "~3.6.3"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "volta": {
    "node": "12.12.0",
    "yarn": "1.19.1"
  }
}
